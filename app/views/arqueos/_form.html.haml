-# frozen_string_literal: true
= simple_form_for(@arqueo) do |f|
  = f.input :monto_cajero
  = f.input :observacion

  .btn-group.float-right
    = link_to arqueos_path, class: 'btn btn-sm btn-secondary' do
      %i.fa.fa-arrow-left
      Regresar
    = f.button :submit, class: 'btn btn-sm btn-primary'
